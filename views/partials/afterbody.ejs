<script>
  let htmlClasses = document.querySelector("html").classList;

  if (
    (window.matchMedia &&
      window.matchMedia("(prefers-color-scheme: dark)").matches) ||
    localStorage.theme === "dark"
  ) {
    htmlClasses.remove("dark");
    localStorage.removeItem("theme");
    document.querySelectorAll("body > .svg").forEach((svgEl) => {
      svgEl.style.filter = "invert(0%)";
    });
    document.querySelector("#theme-switch").innerHTML = "â˜€ï¸";
  } else {
    htmlClasses.add("dark");
    localStorage.theme = "dark";
    document.querySelectorAll("body.dark > .svg").forEach((svgEl) => {
      svgEl.style.filter = "invert(100%)";
    });
    document.querySelector("#theme-switch").innerHTML = "ðŸŒ™";
  }

  function switchMode(el) {
    if (localStorage.theme === "dark") {
      htmlClasses.remove("dark");
      localStorage.removeItem("theme");
      document.querySelectorAll("body > .svg").forEach((svgEl) => {
        svgEl.style.filter = "invert(0%)";
      });
      document.querySelector("#theme-switch").innerHTML = "â˜€ï¸";
    } else {
      htmlClasses.add("dark");
      localStorage.theme = "dark";
      document.querySelector("#theme-switch").innerHTML = "ðŸŒ™";
      document.querySelectorAll("body.dark > .svg").forEach((svgEl) => {
        svgEl.style.filter = "invert(100%)";
      });
    }
  }

  function loadTheme() {
    if (
      localStorage.theme === "dark" ||
      (!"theme" in localStorage &&
        window.matchMedia("(prefers-color-scheme: dark)").matches)
    ) {
      document.querySelector("html").classList.add("dark");
      document.querySelectorAll("body.dark > .svg").forEach((svgEl) => {
        svgEl.style.filter = "invert(100%)";
      });
      document.querySelector("#theme-switch").innerHTML = "ðŸŒ™";
    } else if (localStorage.theme === "dark") {
      document.querySelector("html").classList.add("dark");
      document.querySelectorAll("body.dark > .svg").forEach((svgEl) => {
        svgEl.style.filter = "invert(100%)";
      });
      document.querySelector("#theme-switch").innerHTML = "ðŸŒ™";
    }
  }

  window.onload = loadTheme();
</script>
<script>
  function initFingerprintJS() {
    // Initialize an agent at application startup.
    const fpPromise = FingerprintJS.load();

    // Get the visitor identifier when you need it.
    fpPromise
      .then((fp) => fp.get())
      .then((result) => {
        // This is the visitor identifier:
        const visitorId = result.visitorId;
        console.log(visitorId);
      });
  }
</script>
